class Country {
  String source;
  String target;
  int weight;

  Country() {
    source = "source";
    target = "target";
    weight = 0;
  }
  
  void setSource(String _source){
    source = _source;
  }
  
  void setTarget(String _target){
    target = _target;
  }
  
  void setWeight(int _weight){
    weight = _weight;
  }
}

